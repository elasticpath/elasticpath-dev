{
  "rewrites": [
    {
      "source": "/docs/self-managed/composer/:path*",
      "destination": "https://self-managed-composer.vercel.app/:path*"
    },
    {
      "source": "/docs/composer/:path*",
      "destination": "https://turbo-composer.vercel.app/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
